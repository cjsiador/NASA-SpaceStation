        <engine:VisualElement name="ExitContainer" style="flex-grow: 0; flex-direction: row; align-items: auto; justify-content: center; height: 10%;">
            <engine:Button text="Exit" style="height: 10%; font-size: 14px; position: relative; max-height: 10%; min-height: auto; min-width: 150px;" />
        </engine:VisualElement>
    </engine:VisualElement>
